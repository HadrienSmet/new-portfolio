// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Projects {
  id Int @id @default(autoincrement())
  name String
  images String[]
  legends String[]
  image_link String
  description String
  tools String[]
  code_link String
  code_server_link String?
  link String?
}
